{
	"name": "leaguessify",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "npm run mapData && svelte-kit build",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. .",
		"mapData": "node ./scripts/championMap.cjs && node ./scripts/itemMap.cjs && node ./scripts/runesReforgedMap.cjs && node ./scripts/summonerMap.cjs"
	},
	"devDependencies": {
		"@sveltejs/adapter-netlify": "^1.0.0-next.17",
		"@sveltejs/kit": "next",
		"@types/ioredis": "^4.27.1",
		"@typescript-eslint/eslint-plugin": "^4.30.0",
		"@typescript-eslint/parser": "^4.30.0",
		"autoprefixer": "^10.3.3",
		"cssnano": "^5.0.8",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.0",
		"ioredis": "^4.27.9",
		"postcss": "^8.3.6",
		"postcss-load-config": "^3.1.0",
		"prettier": "~2.3.2",
		"prettier-plugin-svelte": "^2.4.0",
		"svelte": "^3.42.4",
		"svelte-check": "^2.2.5",
		"svelte-preprocess": "^4.8.0",
		"tailwindcss": "^2.2.9",
		"tslib": "^2.3.1",
		"typescript": "^4.4.2"
	},
	"type": "module",
	"dependencies": {
		"svelte-icons": "^2.1.0",
		"tailwindcss-neumorphism": "^0.1.0",
		"tippy.js": "^6.3.1"
	}
}
